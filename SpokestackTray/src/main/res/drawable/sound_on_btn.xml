<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--    <item android:drawable="@drawable/sound_circle"/>-->
    <item>
        <shape android:shape="oval">
            <size
                android:width="25dp"
                android:height="25dp" />
            <solid android:color="@color/spsk_colorBrand" />
            <stroke
                android:width="1dp"
                android:color="@color/spsk_colorBrand" />
        </shape>
    </item>
    <item>
        <inset
            android:drawable="@drawable/sound_on"
            android:insetLeft="8dp"
            android:insetTop="11dp"
            android:insetRight="11dp"
            android:insetBottom="11dp" />
    </item>

</layer-list>